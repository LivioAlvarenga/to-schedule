{
  "name": "template-next-app-fullstack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "studio": "npx prisma studio -b chrome -p 5173",
    "generate": "npx prisma generate",
    "migrate": "npx prisma migrate dev",
    "seed": "npx prisma db seed",
    "start-docker": "docker-compose up -d",
    "stop-docker": "docker-compose stop",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "test:ui": "vitest --ui --no-threads",
    "test:coverage": "vitest run --coverage --no-threads",
    "test:e2e": "vitest run --dir src/http/controllers --no-threads",
    "test:safety": "vitest run --dir src/http/middlewares"
  },
  "dependencies": {
    "@prisma/client": "5.1.1",
    "axios": "1.4.0",
    "dotenv": "16.3.1",
    "next": "13.4.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "3.21.4",
    "zod-validation-error": "1.3.1"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "2.1.0",
    "@types/node": "20.4.10",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@types/supertest": "^2.0.12",
    "@vitest/coverage-v8": "^0.34.1",
    "@vitest/ui": "^0.34.1",
    "autoprefixer": "10.4.14",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.13",
    "postcss": "8.4.27",
    "prettier-plugin-tailwindcss": "0.5.2",
    "prisma": "5.1.1",
    "supertest": "^6.3.3",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.1"
  }
}
